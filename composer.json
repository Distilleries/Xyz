{
  "name": "distilleries/xyz",
  "description": "X.y.z is a fresh instance of laravel 5.* with Expendable package configured.",
  "keywords": [
    "framework",
    "laravel",
    "expendable",
    "distilleries",
    "gulp",
    "sass"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "laravel/framework": "5.0.*",
    "distilleries/expendable": "2.0.*"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0",
    "phpspec/phpspec": "~2.1",
    "fzaninotto/faker": "1.4.*"
  },
  "autoload": {
    "classmap": [
      "database"
    ],
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/TestCase.php"
    ]
  },
  "scripts": {
    "post-install-cmd": [
      "php artisan clear-compiled",
      "php artisan optimize"
    ],
    "post-update-cmd": [
      "php artisan down",
      "php artisan clear-compiled",
      "php artisan optimize",
      "php artisan vendor:publish",
      "php artisan migrate --force",
      "npm install",
      "gulp",
      "php artisan up"
    ],
    "post-create-project-cmd": [
      "php -r \"copy('.env.example', '.env');\"",
      "php artisan key:generate",
      "php artisan vendor:publish"
    ]
  },
  "config": {
    "preferred-install": "dist"
  },
  "minimum-stability": "stable"
}
