{
  "name": "distilleries/xyz",
  "description": "Empty project.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "authors": [
    {
      "name": "Maxime Francois",
      "email": "kezho@me.com"
    }
  ],
  "license": "MIT",
  "type": "project",
  "version": "1.0.0",
  "repositories": [
  ],
  "require": {
    "php": ">=5.5.0",
    "laravel/framework": "4.2.*",
    "distilleries/expendable": "1.*"
  },
  "require-dev": {
    "way/generators": "~2.0",
    "fzaninotto/faker": "1.4.*",
    "codeception/codeception": "*"
  },
  "autoload": {
    "classmap": [
      "app/commands",
      "app/controllers",
      "app/models",
      "app/database/migrations",
      "app/database/seeds",
      "app/tests/TestCase.php"
    ],
    "psr-4": {
      "Xyz\\": "app/Xyz"
    }
  },
  "scripts": {
    "post-install-cmd": [
      "php artisan clear-compiled",
      "php artisan optimize",
      "php artisan down",
      "npm install",
      "gulp",
      "php artisan up"
    ],
    "post-update-cmd": [
      "php artisan clear-compiled",
      "php artisan optimize",
      "php artisan down",
      "npm install",
      "php artisan migrate --package='distilleries/expendable' --force",
      "php artisan migrate --force",
      "gulp",
      "php artisan up"
    ],
    "post-create-project-cmd": [
      "php artisan key:generate"
    ]
  },
  "config": {
    "preferred-install": "dist"
  },

  "prefer-stable": true,
  "minimum-stability": "dev"
}
